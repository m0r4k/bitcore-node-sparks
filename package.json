{
  "name": "@sparksevo/sparkscore-node",
  "description": "Full node with extended capabilities using Sparkscore and Sparks Core (sparksd)",
  "author": "BitPay <dev@bitpay.com>",
  "version": "5.0.2",
  "main": "./index.js",
  "repository": "git://github.com/sparksevo/sparkscore-node.git",
  "homepage": "https://github.com/sparksevo/sparkscore-node",
  "bugs": {
    "url": "https://github.com/sparksevo/sparkscore-node/issues"
  },
  "contributors": [
    {
      "name": "Christopher Jeffrey"
    },
    {
      "name": "Braydon Fuller",
      "email": "braydon@bitpay.com"
    },
    {
      "name": "Chris Kleeschulte",
      "email": "chrisk@bitpay.com"
    },
    {
      "name": "Patrick Nagurny",
      "email": "patrick@bitpay.com"
    }
  ],
  "bin": {
    "sparkscore-node": "./bin/sparkscore-node"
  },
  "scripts": {
    "test": "mocha -R spec --recursive",
    "regtest": "./regtest/regtest",
    "jshint": "jshint --reporter=node_modules/jshint-stylish ./lib",
    "coverage": "istanbul cover _mocha -- --recursive",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --recursive -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "mocha": "./node_modules/.bin/mocha",
    "check-package": "npm run check-package:name && npm run check-package:version",
    "check-package:name": "test $(jq -r .name package.json) = $(jq -r .name package-lock.json)",
    "check-package:version": "test $(jq -r .version package.json) = $(jq -r .version package-lock.json)",
    "sparksd_test": "./node_modules/.bin/mocha test/services/sparksd.unit.js",
    "install-api" :"npm install git+https://m0r4k:zLrBFb3dfCsCeUCAZrFF@bitbucket.org/m0r4k/insight-api-sparks.git#development",
    "install-ui": "npm install git+https://m0r4k:zLrBFb3dfCsCeUCAZrFF@bitbucket.org/m0r4k/insight-ui-sparks.git#development"
  },
  "tags": [
    "sparks",
    "sparksd"
  ],
  "dependencies": {
    "@sparksevo/insight-api-sparks": "git+https://m0r4k:zLrBFb3dfCsCeUCAZrFF@bitbucket.org/m0r4k/insight-api-sparks.git#development",
    "@sparksevo/insight-ui-sparks": "git+https://m0r4k:zLrBFb3dfCsCeUCAZrFF@bitbucket.org/m0r4k/insight-ui-sparks.git#development",
    "@sparksevo/sparkscore-lib": "git+https://m0r4k:zLrBFb3dfCsCeUCAZrFF@bitbucket.org/m0r4k/sparkscore-lib.git#0.16.8",
    "@sparksevo/sparksd-rpc": "git+https://m0r4k:zLrBFb3dfCsCeUCAZrFF@bitbucket.org/m0r4k/sparksd-rpc.git#development",
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "colors": "^1.3.3",
    "commander": "^2.19.0",
    "errno": "^0.1.7",
    "express": "^4.16.4",
    "liftoff": "^2.5.0",
    "lru-cache": "^5.1.1",
    "mkdirp": "0.5.1",
    "path-is-absolute": "^2.0.0",
    "semver": "^5.6.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "zeromq": "^5.1.0"
  },
  "optionalDependencies": {
    "bufferutil": "~4.0.1",
    "utf-8-validate": "~5.0.2"
  },
  "devDependencies": {
    "@sparksevo/sparkscore-p2p": "git+https://m0r4k:zLrBFb3dfCsCeUCAZrFF@bitbucket.org/m0r4k/sparkscore-p2p.git#development",
    "benchmark": "2.1.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "jshint": "^2.10.1",
    "jshint-stylish": "^2.2.1",
    "mocha": "^5.2.0",
    "proxyquire": "^2.1.0",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.3"
  },
  "license": "MIT"
}
